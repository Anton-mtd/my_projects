<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefWidth="250.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.skomorokhin.cloudClient.controllers.DirWinClientController">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="choose directory" textAlignment="CENTER" />
        <TextField fx:id="directory" alignment="TOP_LEFT" />
        <Button mnemonicParsing="false" onMouseClicked="#pushOk" text="OK" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
